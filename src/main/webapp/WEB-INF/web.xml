<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<!-- Imgur Controllers -->


	<!-- DeviantArt Controllers -->

	<servlet>
		<description></description>
		<display-name>DeviantArtStatsController</display-name>
		<servlet-name>DeviantArtStatsController</servlet-name>
		<servlet-class>aiss.controller.DeviantArtStatsController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeviantArtStatsController</servlet-name>
		<url-pattern>/DeviantArtStatsController</url-pattern>
	</servlet-mapping>

	<!-- Imgur Oauth -->
	<servlet>
		<description></description>
		<servlet-name>ImgurAuthController</servlet-name>
		<servlet-class>aiss.controller.oauth.GenericAuthController</servlet-class>
		<init-param>
			<param-name>provider</param-name>
			<param-value>Imgur</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>ImgurAuthController</servlet-name>
		<url-pattern>/AuthController/Imgur</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<servlet-name>ImgurOAuth2Callback</servlet-name>
		<servlet-class>aiss.controller.oauth.OAuth2Callback</servlet-class>
		<init-param>
			<param-name>provider</param-name>
			<param-value>Imgur</param-value>
		</init-param>
		<init-param>
			<param-name>onSuccess</param-name>
			<param-value>redirect:index.html</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>ImgurOAuth2Callback</servlet-name>
		<url-pattern>/oauth2callback/Imgur</url-pattern>
	</servlet-mapping>

	<!-- DeviantArt Oauth -->
	<servlet>
		<description></description>
		<servlet-name>DeviantArtAuthController</servlet-name>
		<servlet-class>aiss.controller.oauth.GenericAuthController</servlet-class>
		<init-param>
			<param-name>provider</param-name>
			<param-value>DeviantArt</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeviantArtAuthController</servlet-name>
		<url-pattern>/AuthController/DeviantArt</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<servlet-name>DeviantArtOAuth2Callback</servlet-name>
		<servlet-class>aiss.controller.oauth.OAuth2Callback</servlet-class>
		<init-param>
			<param-name>provider</param-name>
			<param-value>DeviantArt</param-value>
		</init-param>
		<init-param>
			<param-name>onSuccess</param-name>
			<param-value>redirect:index.html</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeviantArtOAuth2Callback</servlet-name>
		<url-pattern>/oauth2callback/DeviantArt</url-pattern>
	</servlet-mapping>

	<!-- Unsplash Oauth -->
	<servlet>
		<description></description>
		<servlet-name>UnsplashAuthController</servlet-name>
		<servlet-class>aiss.controller.oauth.GenericAuthController</servlet-class>
		<init-param>
			<param-name>provider</param-name>
			<param-value>Unsplash</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>UnsplashAuthController</servlet-name>
		<url-pattern>/AuthController/Unsplash</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<servlet-name>UnsplashOAuth2Callback</servlet-name>
		<servlet-class>aiss.controller.oauth.OAuth2Callback</servlet-class>
		<init-param>
			<param-name>provider</param-name>
			<param-value>Unsplash</param-value>
		</init-param>
		<init-param>
			<param-name>onSuccess</param-name>
			<param-value>redirect:index.html</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>UnsplashOAuth2Callback</servlet-name>
		<url-pattern>/oauth2callback/Unsplash</url-pattern>
	</servlet-mapping>
</web-app>